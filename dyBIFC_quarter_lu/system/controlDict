FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.8;

deltaT          4e-6;

writeControl    adjustableRunTime;

writeInterval   0.001;

purgeWrite      2;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

libs ( "libOpenFOAM.so" "libkOmegaDES.so" ) ;

functions
{
  fieldAverage1
    {
        type            fieldAverage;
        functionObjectLibs ( "libfieldFunctionObjects.so" );
        enabled         true;
        //cleanRestart    true;
        outputControl   outputTime;
        timeStart       0.1;
        timeEnd         0.8;

        fields
        (
            U
            {
                mean        on;
                prime2Mean  on;
                base        time;
            }
            
            p
            {
                mean        on;
                prime2Mean  on;
                base        time;
            }
            Cbound
            {
                mean        on;
                prime2Mean  off;
                base        time;
            }
            CDES
            {
                mean        on;
                prime2Mean  off;
                base        time;
            }
            k
            {
                mean        on;
                prime2Mean  off;
                base        time;
            }
        );
    }
/* #include "computePrimeDict"
 #include "computeProductionDict"
 #include "computeEpsilonDict"
 #include "computeTransportDict"
 #include "computeGradUMeanDict"
 #include "computePijDict"
 #include "computeGradUPrimeDict"
 #include "computeEijDict"
 #include "computeScalarTDict"
 #include "computeVectorGradTDict"
 #include "computeTijDict"
 #include "computeQijDict"
 #include "computepUPrimeDict"
 #include "computeGradpUPrimeDict"
 #include "computeDijDict"
 #include "computeVijDict"

 //#include "fieldAverageDict2"

 #include "computeMeanMomDict"   */
    probes
    {
        // Where to load it from
        functionObjectLibs ( "libsampling.so" );

        type            probes;

        // Name of the directory for probe data
        name            probes;

        // Write at same frequency as fields
        outputControl   timeStep;
        outputInterval  1;

        // Fields to be probed
        fields
        (
            U UMean p
        );

        probeLocations
        (
            (-0.005 0.095 0.003 )        
       );

    }

}


