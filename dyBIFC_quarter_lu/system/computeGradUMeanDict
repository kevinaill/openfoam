    computeGradUMean
    {
        functionObjectLibs ( "libutilityFunctionObjects.so" );
        enabled         true;
        type            coded;
        redirectType    computeGradUMean;
        //writeControl    writeTime;
        //outputControl   timeStep;
        //outputInterval  5;

        codeOptions
        #{
            -I$(LIB_SRC)/meshTools/lnInclude
        #};

        code
        #{
            const volVectorField& UMean = mesh().lookupObject<volVectorField>("UMean");
	    static autoPtr<volTensorField> tField;
	    
	    if(!tField.valid())
	    {
	        tField.set
      		(
         	    new volTensorField
         	    (
            		IOobject
            		(
               		    "GradUMean",
               		    mesh().time().timeName(),
               		    UMean.mesh(),
               		    IOobject::NO_READ,
               		    IOobject::NO_WRITE
            		),
	    		fvc::grad(UMean)
            		//fvc::div(UMean) * dimensionedScalar("tmp", dimensionSet(0, 2, -2, 0, 0, 0, 0), 1)
         	    )
      		);
	    }

	    volTensorField &GradUMean = tField();
	    GradUMean.checkIn();
	    GradUMean = fvc::grad(UMean);
	#};
    }
